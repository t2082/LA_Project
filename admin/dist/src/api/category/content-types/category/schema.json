{
    "kind": "collectionType",
    "collectionName": "categories",
    "info": {
        "singularName": "category",
        "pluralName": "categories",
        "displayName": "Category"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "color": {
            "type": "string"
        },
        "icon": {
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ],
            "type": "media",
            "multiple": true
        },
        "products": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::product.product",
            "inversedBy": "categories"
        }
    }
}
